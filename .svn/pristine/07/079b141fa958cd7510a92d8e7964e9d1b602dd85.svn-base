body,
html,
div,
ul,
li,
iframe,
dd,
dl,
dt,
p,
img {
    border: none;
    padding: 0;
    margin: 0;
}

html,
body {
    padding: 0;
    margin: 0;
    width: 100%;
    height: 100%;
    overflow: hidden;
}


/**设置滚动条的样式**/

::-webkit-scrollbar {
    width: 8px;
}


/**滚动槽**/

::-webkit-scrollbar-track {
    box-shadow: inset 0 0 4px #d1cfcf;
    border-radius: 8px;
}


/**滚动条滑块**/

::-webkit-scrollbar-thumb {
    border-radius: 10px;
    background: #CCCCCC;
    -webkit-box-shadow: inset 0 0 4px #d1cfcf;
}

::-webkit-scrollbar-thumb:window-inactive {
    background: rgba(245, 245, 245, 0.4);
}

.newdoc-header {
    width: 100%;
    height: 8%;
    background-color: #F6F6F6;
    border-bottom: solid 1px #0098dd;
    background-clip: padding-box;
}

.logo {
    width: 100%;
    height: 100%;
    position: relative;
}

.logo img {
    position: absolute;
    top: 50%;
    margin-top: -17px;
}

.newdoc-container {
    height: 92%;
}

.newdoc-row {
    height: 100%;
}

.newdoc-tree {
    height: 100%;
    margin: 0;
    padding: 0;
}

.newdoc-tree-body {
    width: 100%;
    height: 98%;
    overflow-y: scroll;
}

.newdoc-tree-header {
    width: 100%;
    height: 5%;
    background-color: #F6F6F6;
    padding-left: 3px;
    position: relative;
}

.newdoc-tree-header label {
    font-size: 12px;
    color: #000;
    font-family: "微软雅黑";
    position: absolute;
    top: 20%;
}

.newdoc-codes-header {
    width: 100%;
    height: 5%;
    background-color: #F6F6F6;
    padding-left: 3px;
    position: relative;
}

.newdoc-codes-header label {
    font-size: 12px;
    color: #000;
    font-family: "微软雅黑";
    position: absolute;
    top: 20%;
}


