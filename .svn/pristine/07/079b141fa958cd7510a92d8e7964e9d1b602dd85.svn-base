.newdoc-aci-tree {
    width: 100%;
    padding: 0;
    margin: 0;
}

.newdoc-aci-tree ul {
    width: 90%;
    font-size: 14px;
    font-family: "微软雅黑";
}

.list_item {
    list-style: none;
    border-bottom: 1px solid #e9e9e9;
    padding: 15px 10px;
}

.list_item a {
    width: 100%;
    display: block;
    height: 21px;
    text-decoration: none;
    color: #000;
}

.list_item a:hover:focus {
    color: #0075c7 !important;
}

.t_t {
    width: 13px;
    height: 13px;
    display: block;
    float: left;
    margin-top: 4px;
    margin-right: 7px;
    border: 1px solid #999999;
    border-radius: 2px;
    background-clip: padding-box;
    font-size: 7px;
    font-weight: bold;
    line-height: 13px;
    text-align: center;
}

.t_close {
    /*background: url('../image/icons.png') no-repeat 0 -96px*/
}

.t_close:before {
    content: '\002B';
    color: #000;
}

.t_open {
    /*background: url('../image/icons.png') no-repeat 0 -119px*/
}

.t_open:before {
    content: '\2013';
    color: #000;
}

.submenu {
    display: none;
    width: 100%;
    margin-left: 6px;
    margin-top: 10px;
}

.submenu dl {
    list-style-type: square;
}

.submenu dl dd {
    line-height: 35px;
    height: 35px;
    font-size: 14px;
}

.submenu dl dd:before {
    border-bottom: 1px dotted #b0b5c2;
    content: "";
    display: inline-block;
    float: left;
    width: 10px;
    margin: -15px 0 0;
    height: 33px;
    border-left: 1px dotted #b0b5c2;
}

.submenu dl dd a:before {
    content: "·";
    font-size: 27px;
    float: left;
    margin-left: 2px;
    margin-right: 1px;
}


