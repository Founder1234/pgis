{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["src/jsdoc", "README.md"],
        "includePattern": ".+\\.doc.js?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "template": "./api_theme",
        "encoding": "utf8",
        "destination": "./dist/doc",
        "recurse": true
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "default": {
            "outputSourceFiles": true
        },
        "applicationName": "ClientV7.1 API",
        "disqus": "",
        "googleAnalytics": "",
        "openGraph": {
            "title": "",
            "type": "website",
            "image": "",
            "site_name": "",
            "url": ""
        },
        "meta": {
            "title": "",
            "description": "",
            "keyword": ""
        },
        "linenums": false
    },
    "markdown": {
        "parser": "gfm",
        "hardwrap": true,
        "tags": ["examples"]
    }
}
